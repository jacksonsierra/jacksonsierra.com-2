          | Anonymous messaging board on web, iOS and Android platforms
        ul.list-inline
          li: +tooltip-ios('https://appetize.io/app/x7qywn60g6y0bn7krq2x7tkukw')
          li: +tooltip-github('https://github.com/repl-rebels/murmur-mobile/tree/production', 'Github iOS')
        ul.app-stack
          li.btn.btn-default.app-tech React
          li.btn.btn-default.app-tech React Native
          li.btn.btn-default.app-tech Node.js
          li.btn.btn-default.app-tech Express
          li.btn.btn-default.app-tech Firebase
          li.btn.btn-default.app-tech DigitalOcean
    section.row.app
      img(src='images/kevin-curtis-3308.min.jpg', class="app-image", alt="Couples eating at a restaurant")
