extends layout
include tooltip.pug

block append content
  .app-container.container-overflow
    section.row.app
      img(src='images/symple.min.png', class="app-image", alt="Symple logo")
      .app-info
        h3.symple Symple
        p.app-description
          | Co-founded a YCombinator (W17) startup that is a platform for online business payments
        ul.list-inline
          li: +tooltip-web('https://getsymple.com')
        ul.app-stack
          li.btn.btn-default.app-tech Entrepreneurship
          li.btn.btn-default.app-tech React
          li.btn.btn-default.app-tech Redux
          li.btn.btn-default.app-tech Django/DRF
          li.btn.btn-default.app-tech PostgreSQL
          li.btn.btn-default.app-tech Webpack
          li.btn.btn-default.app-tech AWS
    section.row.app
      img(src='images/stephen-curry.min.jpg', class="app-image", alt="Stephen Curry pumping up the crowd")
      .app-info
        h3.chef-curry Chef Curry
        p.app-description
          | RealÂ­time iOS APNS application to alert when Stephen Curry has caught fire in an NBA game
        ul.list-inline
          li: +tooltip-github('https://github.com/jacksonsierra/chef-curry-ios', 'Github iOS')
          li: +tooltip-ios('https://appetize.io/app/fv22akuam8vdedq3kpx98q43jc')
          li: +tooltip-github('https://github.com/jacksonsierra/chef-curry-backend', 'Github Backend')
        ul.app-stack
          li.btn.btn-default.app-tech Objective-C
          li.btn.btn-default.app-tech Django
          li.btn.btn-default.app-tech PostgreSQL
          li.btn.btn-default.app-tech DigitalOcean
    section.row.app
      img(src='images/immedia-stock.min.png', class="app-image", alt="immedia logo")
      .app-info
        h3.immedia immedia
